ul.timeline
  list-style-type none
  position relative
  &:before
    content ' '
    background #d4d9df
    display inline-block
    position absolute
    left 29px
    width 2px
    height 100%
    z-index 400
  & > li
    margin 20px 0
    padding-left 20px
  & > li:before
    content ' '
    background white
    display inline-block
    position absolute
    border-radius 50%
    border 3px solid #22c0e8
    left 20px
    width 20px
    height 20px
    z-index 400
